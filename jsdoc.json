{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": "\\.(js|jsx)$",
    "excludePattern": "(node_modules|build|public)"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "package": "./package.json"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "default": {
      "outputSourceFiles": true
    }
  }
}
