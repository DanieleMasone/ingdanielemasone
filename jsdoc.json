{
  "source": {
    "include": ["src"],
    "includePattern": "\\.(js|jsx)$",
    "excludePattern": "(node_modules|build)"
  },
  "plugins": ["node_modules/better-docs/component"],
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "README.md",
    "template": "node_modules/better-docs"
  }
}
